                    <!-- Begin Page Content -->
                    <div class="container-fluid">

                        <!-- Content -->

                        <div class="card shadow mb-4 cd">
                            <div class="card-header py-3">
                                <h6 class="m-0 font-weight-bold text-primary" style="font-size:2rem;">Country Add </h6>
                            </div>
                            <div class="card-body cbdy">
                                <div class="table-responsive">
                                    <table class="table table-bordered" id="dataTable" width="100%" cellspacing="0">
                                        <thead>
                                            <form action="/admin/slotBooking_post" method="post">

                                                <% if (failed.length>0) { %>
                                                    <div class="form-group">
                                                        <p class="alert alert-danger mb-2"><%=failed %> </p>
                                                    </div>
                                                <% } %>

                                                <div class="form-outline mb-4 frm_ad">
                                                    <input type="text" id="form5Example1" class="form-control frm_cnt"
                                                        name="name" />
                                                    <label class="form-label" for="form5Example1">Name</label>
                                                </div>

                                                <div class="form-outline mb-4 frm_ad">
                                                    <input type="text" id="form5Example2" class="form-control frm_cnt"
                                                        name="phone" />
                                                    <label class="form-label" for="form5Example2">Phone No.</label>
                                                </div>

                                                <div class="form-outline mb-4 frm_ad">
                                                    <input type="date" id="form5Example2" class="form-control frm_cnt"
                                                        name="slotDate" />
                                                    <label class="form-label" for="form5Example2">Slot Date</label>
                                                </div>
                                                <div class="form-outline mb-4 frm_ad">
                                                    <!-- <input type="text" id="form5Example2" class="form-control frm_cnt"
                                                        name="slotTime" /> -->
                                                    <select name="slotTime" id="form5Example2" class="form-control frm_cnt">
                                                        <option value="10am-11am">10am-11am</option>
                                                        <option value="11am-12am">11am-12am</option>
                                                        <option value="12am-1pm">12am-1pm</option>
                                                        <option value="1pm-2pm">1pm-2pm</option>
                                                    </select>
                                                    <label class="form-label" for="form5Example2">Slot Time</label>
                                                </div>



                                                <!-- Submit button -->
                                                <input type="submit" class="btn btn-primary form-control sub"
                                                    type="submit" value="Submit">

                                            </form>

                                            </tbody>
                                    </table>
                                </div>
                            </div>
                        </div>

                        <!--  -->



